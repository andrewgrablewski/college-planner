<div class="row">
  <div class="col s12">
    <h3 class="center-align">Student Directory</h3>
  </div>
</div>



<div class="row">
  <% @students.each do |student| %>
    <div class="col s6 m6">
      <div class="card">
        <div class="card-content white-text">
          <span class="card-title">Name: <%= student.name %></span>
            <p>Email: <%= student.email %></p>
          <p>Course Enrollments</p>
        </div>
        <div class="card-action">
          <a href="#">View profile</a>
        </div>
      </div>
    </div>
  <% end %>

    </div>
  </div>
